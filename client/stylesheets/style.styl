body
	padding 70px 0 70px 0
	@-moz-document url-prefix()
		fieldset
			display table-cell
	.no-games
		margin 0 0 0 15px
	.officialGame
		margin 20px 0 0 0
		div
			margin 0 0 5px 0
	.coach, .player
		div
			margin 0 0 5px 0
	.footer
		margin 20px 0 0 0
	.removeCoach,
	.removePlayer
		margin 0 0 0 5px
	.actions
		button
			margin 5px 0 0 0
	.nav-pills
		margin 0 0 20px 0
	.inlineClass
		display inline
	.displayReplacement
		margin 10px 0 0 15px
	.doReplacement,
	.validateInPlayPlayers
		margin 10px 0 0 0
	.italic
		font-style italic
	.oT
		margin 0 10px 0 10px
	.yourClubPillForModal
		margin 5px 5px 5px 5px
		background-color #FFCF3A
	.opponentPillForModal
		margin 5px 5px 5px 5px
		background-color #FF753A
	.modal-body
		margin 0 10px 0 10px
	.table-responsive
		margin 20px 0 0 0
	.score
		margin 0 0 20px 0
	.yourClubPlayers > .inPlay,
	.opponentPlayers > .inPlay
		background-color #ADF5FF
	#yourClubOnTheBench,
	#yourClubInPlay,
	#opponentInPlay,
	#opponentOnTheBench
		margin 20px 0 20px 0
		border-radius 2px
		padding 5px
		min-height 54px
	.modal-body > .input-group
		margin 0 0 10px 0
	.coach div,
	.gameInfos div,
	.player div
		margin	0 0 5px 0
	.players,
	.coachs
		margin 5px
	.loginInfos div,
	.personalInfos div
		margin 0 0 5px 0
	.loginInfos
		margin 20px 0 0 0
	.personalInfos
		margin 40px 0 0 0
	.deleteUser
		margin 60px 0 0 0
	form div
		margin 0 0 5px 0
	#validateClubCreation
		margin 10px 0 0 0








