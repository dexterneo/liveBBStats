template(name='playerBio')
	with gameData
		+gameInfos
		.panel.panel-default
			.panel-heading
				h4 {{_ "playerDefinition"}} : #{gameInfos.yourClub}
			.panel-body
				.players.row
					each playerData
						+playerUpdate
		.panel.panel-default
			.panel-heading
				h4.panel-title {{_ "coachDefinition"}} : #{gameInfos.yourClub}
			.panel-body
				.coachs.row
					each coachData
						+coachUpdate